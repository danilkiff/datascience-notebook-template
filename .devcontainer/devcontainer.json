{
  "name": "DS Notebook",
  "dockerComposeFile": ["../docker-compose.yaml"],
  "service": "jupyter",
  "workspaceFolder": "/home/jupyternb",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "charliermarsh.ruff"
      ],
      "settings": {
        "python.defaultInterpreterPath": "/opt/conda/bin/python"
      }
    }
  },
  "shutdownAction": "none"
}
